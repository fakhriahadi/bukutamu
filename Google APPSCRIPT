//ini untuk versi 2.1

function Loket1() {
  var sheet = SpreadsheetApp.getActiveSpreadsheet()
  var SCurrent = sheet.getSheetByName("Loket1");
  var SForm = sheet.getSheetByName("Form Responses 1");

  var selF5 = SCurrent.getRange('F5');
  var nilaiF5 = selF5.getValue();

  if (nilaiF5 > "0") {
  var nilaiTujuan2 = "J" + nilaiF5;
  var selTujuan2 = SForm.getRange(nilaiTujuan2);
  selTujuan2.setValue("F");};

  var selF11 = SCurrent.getRange('F11');
  var nilaiF11 = selF11.getValue();
  
  var nilaiTujuan1 = "J" + nilaiF11;
  var selTujuan1 = SForm.getRange(nilaiTujuan1);

  selTujuan1.setValue("1");

}
